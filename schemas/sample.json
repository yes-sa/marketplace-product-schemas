{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "definitions": {
        "properties": {
            "name": {
                "type": "string",
                "maxLength": 100
            },
            "images": {
                "type": "array",
                "minItems": 1,
                "items": {
                    "type": "object"
                }
            }
        }
    },
    "required": [
        "name",
        "images"
    ]
}